<script>
    function getElementOffset(elem) {
        var top=0, left=0;
        while(elem) {
            top = top + parseInt(elem.offsetTop);
            left = left + parseInt(elem.offsetLeft);
            elem = elem.offsetParent;
        }
        return {top: top, left: left}
    }
    function setPosition() {
        var line = getElementOffset(document.getElementsByClassName('header-subtitle')[0]);
        var leftPX = line.left;
        var topScroll = window.pageYOffset || document.documentElement.scrollTop;
        var topPX = line.top - topScroll + 35;
        var sheet = document.styleSheets[3];
        var rules = sheet.cssRules || sheet.rules;
        rules[0].style.top = topPX + 'px';
        rules[0].style.left = leftPX + 'px';
    }
    function setEvents(className, redirectLink){
        var elem = document.getElementsByClassName(className)[0];
        if (elem != undefined) {
            elem.onclick = function () {
                window.location.href = redirectLink;
            };            
            window.onscroll = function() {
                setPosition(); 
            };
            window.onresize = function(){
                setPosition();
            };
            
            setPosition(); 
            
        } else {
            console.log('Element not found');
        }
    }
