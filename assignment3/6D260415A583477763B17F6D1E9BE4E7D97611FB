            if (id == 'uGmpdvqRXWDEihAMlsFQSe') name = 'ad_usenet';
            if (id == 'JknowbqXMCIScrVUx') name = 'ad_sponsored';

            if (send_ga_events != undefined && send_ga_events)
                ga('send', 'event', 'ad_clicked', 'click', name);
        }
